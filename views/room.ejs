    </script>
</head>
<body>
     <div id="total">
        <div id="vidblock">
            <div id="video-grid" >

            </div>
           <div id="spacebottom">

             <div id="options">
               <div class="icon1" onclick="vidonandoff()" id="elements" >
           <i class="fas fa-video"  ></i>
                </div>
                <div class="icon2" onclick="muteonandoff()" id="elements" >
          <i class="fas fa-microphone " ></i>
          </div>
          <div class="hangup"  id="elements" onclick="showChat()">
            <i class="fas fa-comment " id="hang" ></i>
            </div>
          </div>
         </div>
          </div>
           <div id="msgblock">
            <h1 class="heading">Meeting Chat</h1>
            <div id ="txtinput">
              <ul class="everymsg"></ul>
            <input class="input" type="text" name="" value="" placeholder="send a message...">
            <button type="button" name="button">send</button>
          </div>

      </div>

    </div>

  

    <script src="script.js"></script>
